{
    "manifest_version": 3,
    "name": "Drive Embedded Link Generator",
    "version": "7.0",
    "description": "Kapoy Mag Capcha. Dev by STI OJT's Yr 2025.",
    "permissions": ["activeTab"],
    "icons": {
      "128": "icon.png"
    },
    "action": {
    
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://drive.google.com/drive*"],
        "js": ["inject.js"],
        "run_at": "document_end"
      }
    ]
  }
